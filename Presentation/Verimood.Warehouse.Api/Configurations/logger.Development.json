{
  "Serilog": {
    "Using": [ "Serilog.Sinks.PostgreSQL" ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "WithHangfireContext"
    ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Verimood.Warehouse.Application": "Information"
      }
    },
    "Properties": {
      "Application": "Verimood.Warehouse.Service"
    },
    "WriteTo": [
      {
        "Name": "PostgreSQL",
        "Args": {
          "ConnectionString": "Host=31.40.197.73;Port=5432;Database=warehouse_db;Username=postgres;Password=Admin123!;Pooling=true;Minimum Pool Size=0;Maximum Pool Size=100;",
          "TableName": "_log",
          "autoCreateSqlTable": true,
          "storeTimestampInUtc": true
        }
      }
    ]
  }
}