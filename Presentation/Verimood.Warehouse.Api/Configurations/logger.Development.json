{
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "WithHangfireContext"
    ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Werimood.Warehouse.Application": "Information"
      }
    },
    "Properties": {
      "Application": "Verimood.Warehouse"
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "ConnectionString": "Server=localhost,1433;Database=VerimoodWarehouse;User Id=sa;Password=Admin123!;TrustServerCertificate=True;",
          "TableName": "_log",
          "autoCreateSqlTable": true,
          "storeTimestampInUtc": true
        }
      }
    ]
  }
}